{
    "name": "crypto-markets/binance",
    "description": "Binance gateway for the cryptocurrency processing library.",
    "keywords": ["cryptocurrency", "exchange", "binance"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sercan Cakir",
            "email": "srcnckr@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "crypto-markets/common": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "CryptoMarkets\\Binance\\": "src/"
        }
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "CryptoMarkets\\Tests\\Binance\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
